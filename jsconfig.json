{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      //   "src/pages/*": ["pages/*"]
      "src/*": ["./*"]
    },
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "allowJs": true
  },
  "include": ["src/**/*", "src"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
